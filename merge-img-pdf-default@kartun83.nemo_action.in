[Nemo Action]
Active=true
Name=Merge to PDF (Default)
Comment=Merge selected images to a PDF with default settings
Exec=<merge-img-pdf@kartun83/img2pdf_default.sh %F>
Icon-Name=application-pdf
Selection=notnone
Extensions=png;jpg;jpeg;tiff;bmp
Quote=double
Dependencies=yad;img2pdf

