[Nemo Action]
Active=true

Name=Merge to PDF (img2png)
Comment=Merge selected images to a PDF with custom settings
Exec=<merge-img-pdf@kartun83/img2pdf_gui.sh %F>
Icon-Name=application-pdf
Selection=notnone
Extensions=png;jpg;jpeg;tiff;bmp
Quote=double
Dependencies=yad;img2pdf;convert

